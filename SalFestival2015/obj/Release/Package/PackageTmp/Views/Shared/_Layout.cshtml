    <!-- photo gallery-->
    <script>
        $('.gallery_more').click(function () {
            var $this = $(this);
            $this.toggleClass('gallery_more');
            if ($this.hasClass('gallery_more')) {
                $this.text('▼المزيد');
            } else {
                $this.text('▲ أعلى');
            }
        });
    </script>

    <!--side menu-->
    <script>
        $(document).ready(function () {
            $(".sidemenu-bar").toggleClass("open");
            $(".menu-button").click(function () {
                $(".sidemenu-bar").toggleClass("open");
            })
        })
    </script>

    <!-- Head Lines -->
