@model SalFestival2015.Models.tbl_posts

@{
    ViewBag.Title = "تفاصيل المقالة / الخبر";
}

<div class="row">
    <div class="container">
        <div class="col-md-12">
            <div class="artimg">
                <h2>@Model.post_title</h2>
                <img src='@Model.post_img' alt='@Model.post_title'>
            </div>
            <div class="artdesc">
                @*@Html.Raw(HttpUtility.HtmlDecode(Model.post_content));*@
                @*@(new HtmlString(Model.post_content))*@
             
                    @Html.Raw(Model.post_content)
             

               
            </div>
        </div>
    </div>
</div>
